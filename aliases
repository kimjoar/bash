# cd
alias :='cd ..'
alias ::='cd ../..'
alias :::='cd ../../..'
alias p='cd ~/Documents/Projects'

# grep
alias grep='grep --color'

# ls
alias l="ls -l"
alias ll="ls -al"

# Rails
alias ss='./script/server'
alias sc='./script/console'
alias rst='touch tmp/restart.txt'

# ssh
alias subsys='ssh kimjoar@server2.subsys.no'